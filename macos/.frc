# -*- mode: conf -*-
# ======================== F alias ========================

alias finfo='echo "=== alias f ==="
echo ""
echo "-- fd, fasd, 查照文件或目录，支持正则"
echo ""
echo "   fdd    = fd --type directory,      只匹配目录"
echo "   fdf    = fd --type file,           只匹配文件"
echo "   fde    = fd --type empty,          匹配空目录或文件"
echo "   fdE    = fd --extension <ext>,     根据后缀过滤"
echo "   fdp    = fd --type pipe,           管道文件"
echo "   fdx    = fd --type executable,     可执行文件"
echo "   fdh    = fd -h,                    简要帮助信息"
echo "   fdh-   = fd --help,                帮助信息"
echo "   fdH    = fd -H,                    显示隐藏文件"
echo "   fdv    = fd --version,             显示 fd 版本信息"
echo "   xxxi   = fd -x -i, --ignore-case   忽略大小写"
echo "   xxxI   = fd -I,                    包涵 .gitignore, .ignore, .fdignore 中忽略的文件或目录"
echo "   xxxx   = fd ... -x, --exec <cmd>   对每项结果执行 <cmd> 命令"
echo "   xxxX   = fd ... -X, --exec-batch <cmd>   对所有结果执行 <cmd> 命令"
echo "------------------------------------------------------------"
echo ""
echo "-- fe, frontend search, 前端搜索"
echo ""
echo "   feag       = frontend angular"
echo "   feciu      = frontend caniuse"
echo "   fecode     = frontend codepen"
echo "   fepdoc     = frontend compassdoc"
echo "   fecss      = frontend cssflow"
echo "   fedart     = frontend dartlang"
echo "   fegit      = frontend github"
echo "   feh5       = frontend html5please"
echo "   fejq       = frontend jquery"
echo "   felodash   = frontend lodash"
echo "   femdn      = frontend mnd"
echo "   fenode     = frontend nodejs"
echo "   fenpm      = frontend npmjs"
echo "   fereact    = frontend react"
echo "   festack    = frontend stackoverflow"
echo "   fets       = frontend typescript"
echo "   fevue      = frontend vuejs"
echo "------------------------------------------------------------"
echo ""

echo "-- fix, fix, 修复一些问题"
echo ""
echo "   fixcp  = $GCLRC_SHELL/fix.sh chrome-ports, 修复端口限制"
echo "------------------------------------------------------------"
'

alias fdd="fd -t d"
alias fdf="fd -t f"
alias fde="fd -t e"
alias fdp="fd -t p"
alias fdx="fd -t x"
alias fdE="fd -e"
alias fdh="fd -h"
alias fdh-="fd --help"
alias fdH="fd -H"
alias fdv="fd --version"

# 查找应用对应的包名
alias fdfcom="cd $HOME/Library/Preferences && fdf"

# frontend search
# https://github.com/ohmyzsh/ohmyzsh/tree/master/plugins/frontend-search
alias feag="frontend angular"
alias feciu="frontend caniuse"
alias fecode="frontend codepen"
alias fepdoc="frontend compassdoc"
alias fecss="frontend cssflow"
alias fedart="frontend dartlang"
alias fegit="frontend github"
alias feh5="frontend html5please"
alias fejq="frontend jquery"
alias felodash="frontend lodash"
alias femdn="frontend mdn"
alias fenode="frontend nodejs"
alias fenpm="frontend npmjs"
alias fereact="frontend react"
alias festack="frontend stackoverflow"
alias fets="frontend typescript"
alias fevue="frontend vuejs"

# fix
alias fixcp="$GCLRC_SHELL/gfix chrome-ports"

alias flushdns="dscacheutil -flushcache && killall -HUP mDNSResponder"
