echo ""
echo "------------------------------------------------------------"
echo " -- chm, chmod"
echo ""
echo "    chmshl        = cd shl/ && chmod +x <sh-file>"
echo "------------------------------------------------------------"
echo ""
echo "-- cp, copy file"
echo "   cpvr       = ~/.gclrc/bin/gstart vue-reactivity"
echo "   cpvc       = ~/.gclrc/bin/gstart vue vue-compiler-core"
echo "   cpvd       = ~/.gclrc/bin/gstart vue vue-compiler-dom"
echo "   cpvs       = ~/.gclrc/bin/gstart vue vue-compiler-sfc"
